<script>
  window.onload = function () {
    console.log("DOM is loading");
    getImage();
  };

  function getImage() {
    console.log("funktionen k√∂rs");
    fetch(
      "https://api.unsplash.com/photos/random?client_id=" +
        "<%=apiKey%>" +
        "&orientation=landscape&query=australia&featured=true&auto=format,enhance,compress&color=black"
    )
      .then((response) => {
        return response.json();
      })
      .then((data) => {
        let outputContainer = document.body;
        outputContainer.style.backgroundImage =
          "url('" + data.urls.raw + "&w=800&dpr=2";
        ("')");
      });
  }
</script>
